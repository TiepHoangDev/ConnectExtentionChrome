{
  "manifest_version": 2,

  "name": "GET PAGE REQUEST",
  "description": "Capture all request of page and send to server.",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "declarativeContent",
    "alarms",
    "contextMenus",
    "storage",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  }
}